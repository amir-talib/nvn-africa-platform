# Frontend Environment Variables

# API URL - point to your backend server
VITE_API_URL=http://localhost:5000/api

# Cloudinary (for direct uploads from frontend)
# Same credentials as server
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=unsigned_preset_name

# Paystack Public Key (for frontend payment widget)
VITE_PAYSTACK_PUBLIC_KEY=pk_test_your_paystack_public_key
